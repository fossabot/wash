#
# Create pyramid plot for entire survey area
#
pyramid.plot(x = ageDF$ageGrp, g = ageDF$sex,
             main = &quot;&quot;, xlab = &quot;Male     /     Female&quot;,
             ylab = &quot;Age Groups&quot;)
#
# Close graphics device
#
dev.off()

