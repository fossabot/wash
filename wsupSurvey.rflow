################################################################################
#
# Create master data
#
################################################################################
#
# Merge all data.frames from current survey data
#
temp &lt;- merge(adminDF, demoDF, by = &quot;uniqueID&quot;)
temp &lt;- merge(temp, povertyDF, by = &quot;uniqueID&quot;)
temp &lt;- merge(temp, waterDF, by = &quot;uniqueID&quot;)
