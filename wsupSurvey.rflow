  # Cycle through each row of data
  #
  for(i in 1:nrow(surveyData))
    {
    #
    # Populate poverty probabilities vector
    #
    pPoverty &lt;- data.frame(rbind(pPoverty, ppiMatrixGHA[ppiMatrixGHA[&quot;score&quot;] == ppi[i], 2:10]))
    }
  }




